                                    class="rich-item piece piece-<%= item.rarity %>-bg">
                                        <% if(rarityOrder.indexOf(item.rarity) < 3){ %>
                                            <div class="piece-shine"></div>
                                        <% } %>
                                        <div class="piece-hover-area"></div>
                                        <% itemIcon(item, ['piece-icon']); %>
