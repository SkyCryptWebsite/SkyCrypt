                                <% } %>
                                <br>
                                <% for(let i = 0; i < 9; i++){ %>
                                    <% let item = items.inventory[i]; %>
                                    <%= inventorySlot(item); %>
                                <% } %>
                            </div>
                        </div>
                    <% } %>
                </div>
            </div>
